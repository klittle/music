<h1>Editing <%= @artist.name %>  </h1>

<% form_for @artist, :html => {:multipart => true} do |f| %>
	<p>
		<%= f.label :name %>
    <%= f.text_field :name %>
	</p>
		
  <%= f.error_messages %>

  <p>
    <%= f.submit 'Save' %>
  </p>
<% end %>


